{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Information",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "DatabaseSettings": {
    "SettingsDatabase": "settings_db",
    "LoginSettingsCollection": "ldap_settings",
    "AuthenticationDatabase": "auth_db",
    "LocalUserCollection": "local_accounts",
    "DomainUserCollection": "domain_accounts",
    "SessionCollection": "user_sessions",
    "DomainCollection": "auth_domains"
  },
  "EventLog": {
    "LogLevel": {
      "Default": "Information"
    }
  }
}
